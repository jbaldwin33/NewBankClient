<Application x:Class="GrpcGreeterWpfClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GrpcGreeterWpfClient"
             xmlns:viewModels="clr-namespace:GrpcGreeterWpfClient.ViewModels"
             xmlns:views="clr-namespace:GrpcGreeterWpfClient.Views">
  <Application.Resources>
    <!--<ResourceDictionary>
      --><!-- Application styles definition --><!--
      <Style x:Key="Style"/>
    </ResourceDictionary>-->
    <DataTemplate DataType="{x:Type viewModels:HomeViewModel}">
      <views:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:AccountViewModel}">
      <views:AccountView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:LoginViewModel}">
      <views:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:SignUpViewModel}">
      <views:SignUpView/>
    </DataTemplate>
  </Application.Resources>
</Application>
